{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1",
        "EXPO_PUBLIC_API_URL": "https://edt.mularski.pl",
        "ANDROID_HOME": "/Users/pegaz/Library/Android/sdk",
        "EXPO_PUBLIC_TYPE_OF_BUILD": "prod"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "apk": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_USE_METRO_WORKSPACE_ROOT": "1",
          "EXPO_PUBLIC_API_URL": "https://edt.mularski.pl",
          "ANDROID_HOME": "/Users/pegaz/Library/Android/sdk"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_METRO_WORKSPACE_ROOT": "1",
        "EXPO_PUBLIC_API_URL": "https://edt.mularski.pl",
        "ANDROID_HOME": "/Users/pegaz/Library/Android/sdk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
